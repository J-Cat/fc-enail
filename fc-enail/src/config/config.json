{
    "default": {
        "options": {
            "autoShutoff": 1,
            "monitorCycleTime": 1000,
            "displayRefreshRate": 1000,
            "click": {
                "medium": 750,
                "long": 5000,
                "reallyLong": 10000
            },
            "httpPort": 80,
            "httpsPort": 443,
            "httpsPrivateKey": "/home/pi/fc-enail.key",
            "httpsPublicKey": "/home/pi/fc-enail.cert",
            "mDNSName": "FC Community E-Nail",
            "hardware": {
                "oled": {
                    "address": 60
                },
                "button": {
                    "pin": 25,
                    "debounce": 20
                },
                "dial": {
                    "A": 22,
                    "B": 23,
                    "C": 24,
                    "messageThrottle": 250,
                    "rotationThrottle": 250,
                    "rotationAccelerationExpires": 750,
                    "rotationAccelerationThreshold": 250,
                    "rotationMaxStep": 10,
                    "rotationFixedStep": 0,
                    "rotationDebounce": 40,
                    "buttonDebounce": 20
                }
            },
            "e5cc": {    
                "device": "/dev/ttyUSB0",
                "options": {
                    "baudRate": 57600,
                    "dataBits": 8,
                    "stopBits": 1,
                    "parity": "even"
                }
            }
        },
        "files": {
            "scripts": "/home/pi/enail-scripts.json",
            "savedState": "/home/pi/saved-state.json",
            "savedProfiles": "/home/pi/saved-profiles.json"
        },
        "security": {
            "tokenTitle": "FC E-Nail",
            "privateKey": "/home/pi/fc-enail.key",
            "publicKey": "/home/pi/fc-enail.cert",
            "tempWPASupplicant": "/home/pi/wpa_supplicant.conf"
        },
        "jwtOptions": {
            "algorithm": "RS256",
            "audience": "FC E-Nail",
            "issuer": "FC E-Nail",
            "subject": "FC E-Nail"
        },
        "inputCharacters": "-+_!#$%&*@=^ 0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz/\\,.:;?<>\"'`()[]{}|~"
    }
}
